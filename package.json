{
  "name": "iconify-svg-exporter",
  "version": "1.0.0",
  "description": "Una herramienta para exportar iconos de colecciones Iconify como archivos SVG individuales",
  "type": "module",
  "main": "index.js",
  "bin": {
    "export-icons": "./bin/cli.js"
  },
  "scripts": {
    "start": "node index.js",
    "export": "node index.js",
    "build": "npm run export",
    "dev": "node --watch index.js",
    "test": "node --test test/*.test.js",
    "lint": "eslint *.js",
    "format": "prettier --write *.js",
    "clean": "rimraf icons",
    "postinstall": "echo '✨ Instalación completada! Ejecuta: npm run export'"
  },
  "keywords": [
    "iconify",
    "svg",
    "icons",
    "export",
    "batch",
    "converter",
    "graphics",
    "design-tools"
  ],
  "author": {
    "name": "Tu Nombre",
    "email": "tu.email@ejemplo.com",
    "url": "https://tu-sitio-web.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tu-usuario/iconify-svg-exporter.git"
  },
  "bugs": {
    "url": "https://github.com/tu-usuario/iconify-svg-exporter/issues"
  },
  "homepage": "https://github.com/tu-usuario/iconify-svg-exporter#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "@iconify/json": "^2.2.249",
    "@iconify/utils": "^2.1.30"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "eslint": "^9.0.0",
    "prettier": "^3.3.0",
    "rimraf": "^5.0.0"
  },
  "files": [
    "index.js",
    "bin/",
    "README.md",
    "LICENSE"
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/tu-usuario"
  },
  "config": {
    "collections": [
      "nonicons"
    ],
    "outputDir": "./icons",
    "defaultSize": 48,
    "defaultColor": "red"
  }
}